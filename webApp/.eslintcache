password: Yup.string().when('old_password', {\n                  is: (val: any) => !!val.length,\n                  then: Yup.string().required('Campo obrigatório'),\n                  otherwise: Yup.string(),\n               }),\n               password_confirmation: Yup.string()\n                  .when('old_password', {\n                     is: (val: any) => !!val.length,\n                     then: Yup.string().required('Campo obrigatório'),\n                     otherwise: Yup.string(),\n                  })\n                  .oneOf(\n                     [Yup.ref('password'), null],\n                     'Senha de confirmação diferente de senha',\n                  ),\n            });\n\n            await schema.validate(data, { abortEarly: false });\n\n            const {\n               name,\n               email,\n               old_password,\n               password,\n               password_confirmation,\n            } = data;\n            const formData = {\n               name,\n               email,\n               ...(old_password\n                  ? { old_password, password, password_confirmation }\n                  : {}),\n            };\n\n            const response = await api.put('/profile', formData);\n            updateUser(response.data);\n            history.push('/dashboard');\n\n            addToast({\n               type: 'success',\n               title: 'Perfil atualizado!',\n               description: 'Suas informações foram atualizadas com sucesso!',\n            });\n         } catch (err) {\n            if (err instanceof Yup.ValidationError) {\n               const errors = getValidationErrors(err);\n\n               formRef.current?.setErrors(errors);\n\n               return;\n            }\n\n            addToast({\n               type: 'error',\n               title: 'Erro na atualização',\n               description:\n                  'Ocorreu um erro ao fazer atualizar o perfil. Tente novamente.',\n            });\n         }\n      },\n      [addToast, history],\n   );\n\n   const handleAvatarChange = useCallback(\n      (e: ChangeEvent<HTMLInputElement>) => {\n         if (e.target.files) {\n            const data = new FormData();\n\n            data.append('avatar', e.target.files[0]);\n\n            api.patch('/users/avatar', data).then(response => {\n               updateUser(response.data);\n\n               addToast({\n                  type: 'success',\n                  title: 'Avatar atualizado!',\n               });\n            });\n         }\n      },\n      [addToast, updateUser],\n   );\n\n   return (\n      <Container>\n         <header>\n            <div>\n               <Link to=\"/dashboard\">\n                  <FiArrowLeft />\n               </Link>\n            </div>\n         </header>\n         <Content>\n            <Form\n               ref={formRef}\n               initialData={{\n                  name: user.name,\n                  email: user.email,\n               }}\n               onSubmit={handleSubmit}\n            >\n               <AvatarInput>\n                  <img src={user.avatar_url} alt={user.name} />\n                  <label htmlFor=\"avatar\">\n                     <FiCamera />\n                     <input\n                        type=\"file\"\n                        id=\"avatar\"\n                        onChange={handleAvatarChange}\n                     />\n                  </label>\n               </AvatarInput>\n\n               <h1>Meu perfil</h1>\n\n               <Input name=\"name\" icon={FiUser} placeholder=\"Nome\" />\n               <Input name=\"email\" icon={FiMail} placeholder=\"E-mail\" />\n               <Input\n                  containerStyle={{ marginTop: 24 }}\n                  name=\"old_password\"\n                  icon={FiLock}\n                  type=\"password\"\n                  placeholder=\"Senha atual\"\n               />\n\n               <Input\n                  name=\"password\"\n                  icon={FiLock}\n                  type=\"password\"\n                  placeholder=\"Nova senha\"\n               />\n\n               <Input\n                  name=\"password_confirmation\"\n                  icon={FiLock}\n                  type=\"password\"\n                  placeholder=\"Confirmar senha\"\n               />\n\n               <Button type=\"submit\">Confirmar mudanças</Button>\n            </Form>\n         </Content>\n      </Container>\n   );\n};\n\nexport default Profile;\n",["187","188","189","190","191"],"D:\\Personal files\\Learning\\React\\Rocketseat\\GoStack\\GoBarberApp_GoStackBootcamp\\webApp\\src\\routes\\index.tsx",[],"D:\\Personal files\\Learning\\React\\Rocketseat\\GoStack\\GoBarberApp_GoStackBootcamp\\webApp\\src\\pages\\ForgotPassword\\index.tsx",[],"D:\\Personal files\\Learning\\React\\Rocketseat\\GoStack\\GoBarberApp_GoStackBootcamp\\webApp\\src\\pages\\SignUp\\styles.ts",[],"D:\\Personal files\\Learning\\React\\Rocketseat\\GoStack\\GoBarberApp_GoStackBootcamp\\webApp\\src\\components\\Button\\index.tsx",[],"D:\\Personal files\\Learning\\React\\Rocketseat\\GoStack\\GoBarberApp_GoStackBootcamp\\webApp\\src\\components\\Button\\styles.ts",[],"D:\\Personal files\\Learning\\React\\Rocketseat\\GoStack\\GoBarberApp_GoStackBootcamp\\webApp\\src\\components\\Tooltip\\styles.ts",[],{"ruleId":"192","replacedBy":"193"},{"ruleId":"194","replacedBy":"195"},{"ruleId":"196","replacedBy":"197"},{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","replacedBy":"201"},{"ruleId":"192","replacedBy":"202"},{"ruleId":"194","replacedBy":"203"},{"ruleId":"196","replacedBy":"204"},{"ruleId":"198","replacedBy":"205"},{"ruleId":"200","replacedBy":"206"},{"ruleId":"192","replacedBy":"207"},{"ruleId":"194","replacedBy":"208"},{"ruleId":"196","replacedBy":"209"},{"ruleId":"198","replacedBy":"210"},{"ruleId":"200","replacedBy":"211"},{"ruleId":"212","severity":1,"message":"213","line":11,"column":24,"nodeType":"214","messageId":"215","endLine":11,"endCo